{
    "name": "PicUploadTool",
    "description": "Auto upload downloaded picture to GraphBed",
    "version": "0.1.1",
    "options_page": "options.html",
    "background": {
        "scripts": [
            "js/bg.js"
        ],
        "persistent": false
    },
    "permissions": [
        "clipboardWrite",
        "downloads",
        "file:///*",
        "storage"
    ],
    "manifest_version": 2
}